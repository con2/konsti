    "babel-plugin-styled-components": "1.12.0",
    "browserslist": "4.16.1",
    "core-js": "3.8.3",
    "i18next": "19.8.5",
    "i18next-browser-languagedetector": "6.0.1",
    "i18next-xhr-backend": "3.2.2",
    "react": "16.14.0",
    "react-beautiful-dnd": "13.0.0",
    "react-dom": "16.14.0",
    "react-i18next": "11.8.5",
    "react-is": "16.13.1",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-form": "8.3.7",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
