{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "index.tsx",
  "scripts": {
    "build:ci": "yarn clean && webpack-cli",
    "build:dev": "yarn clean && webpack-cli",
    "build:prod": "yarn clean && webpack-cli",
    "build:staging": "yarn clean && webpack-cli",
    "bundle-analyzer": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json build -m server",
    "clean": "yarn run -T rimraf build",
    "eslint": "yarn run -T eslint . --ext .js,.ts,.tsx",
    "eslint-rules": "yarn run -T eslint --print-config .eslintrc.js",
    "start": "webpack serve",
    "stylelint": "stylelint \"./**/*.tsx\"",
    "stylelint-lint-staged": "stylelint",
    "test": "yarn run -T vitest run",
    "test:coverage": "yarn run -T vitest run --coverage --colors",
    "test:watch": "yarn run -T vitest watch",
    "type-check": "yarn run -T tsc --noEmit",
    "playwright": "npx playwright install && npx playwright test --ui --config ./playwright/",
    "playwright:trace": "npx playwright show-trace trace.zip",
    "playwright:debug": "yarn run -T cross-env PWDEBUG=1 npx playwright test --config ./playwright/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@react-hook/intersection-observer": "3.1.1",
    "@reduxjs/toolkit": "1.9.5",
    "@sentry/react": "7.55.2",
    "babel-plugin-styled-components": "2.1.3",
    "browserslist": "4.21.9",
    "core-js": "3.31.0",
    "i18next": "22.4.3",
    "i18next-browser-languagedetector": "7.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.44.3",
    "react-i18next": "12.3.1",
    "react-is": "18.2.0",
    "react-markdown": "8.0.7",
    "react-redux": "8.1.0",
    "react-router-dom": "6.13.0",
    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "regenerator-runtime": "0.13.11",
    "remark-gfm": "3.0.1",
    "styled-components": "5.3.11",
    "use-debounce": "9.0.4"
  },
  "devDependencies": {
    "@axe-core/react": "4.7.3",
    "@playwright/test": "1.35.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@ronilaukkarinen/stylelint-a11y": "1.2.7",
    "@types/dotenv-webpack": "7.0.4",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@types/styled-components": "5.1.26",
    "@welldone-software/why-did-you-render": "7.0.1",
    "babel-loader": "9.1.2",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "10.0.0",
    "doiuse": "4.4.1",
    "dotenv-webpack": "8.0.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.3",
    "jsdom": "22.1.0",
    "postcss": "8.4.24",
    "postcss-styled-syntax": "0.4.0",
    "postcss-syntax": "0.36.2",
    "raw-loader": "4.0.2",
    "react-refresh": "0.14.0",
    "stylelint": "15.7.0",
    "stylelint-config-recommended": "12.0.0",
    "stylelint-no-unsupported-browser-features": "6.1.0",
    "webpack": "5.87.0",
    "webpack-bundle-analyzer": "4.9.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    }
  }
}
