{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "index.tsx",
  "scripts": {
    "build:ci": "yarn clean && webpack-cli",
    "build:dev": "yarn clean && webpack-cli",
    "build:prod": "yarn clean && webpack-cli",
    "build:staging": "yarn clean && webpack-cli",
    "bundle-analyzer": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json build -m server",
    "clean": "yarn run -T rimraf build",
    "eslint": "yarn run -T eslint . --ext .js,.ts,.tsx",
    "eslint-rules": "yarn run -T eslint --print-config .eslintrc.js",
    "start": "webpack serve",
    "stylelint": "stylelint \"./**/*.tsx\"",
    "stylelint-lint-staged": "stylelint",
    "test": "yarn run -T jest",
    "test:coverage": "yarn run -T jest --coverage --colors",
    "test:watch": "yarn run -T jest --watch",
    "type-check": "yarn run -T tsc --noEmit",
    "playwright": "npx playwright install && npx playwright test --config ./playwright/",
    "playwright:trace": "npx playwright show-trace trace.zip",
    "playwright:debug": "yarn run -T cross-env PWDEBUG=1 npx playwright test --config ./playwright/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-regular-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@react-hook/intersection-observer": "3.1.1",
    "@reduxjs/toolkit": "1.8.2",
    "babel-plugin-styled-components": "2.0.7",
    "browserslist": "4.20.4",
    "core-js": "3.22.8",
    "i18next": "21.8.9",
    "i18next-browser-languagedetector": "6.1.4",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "7.31.3",
    "react-i18next": "11.17.0",
    "react-is": "18.1.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "regenerator-runtime": "0.13.9",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@axe-core/react": "4.4.2",
    "@playwright/test": "1.22.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
    "@ronilaukkarinen/stylelint-a11y": "1.2.4",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@types/dotenv-webpack": "7.0.3",
    "@types/moment-locales-webpack-plugin": "1.2.3",
    "@types/react-dom": "18.0.5",
    "@types/styled-components": "5.1.25",
    "@welldone-software/why-did-you-render": "7.0.1",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "10.0.0",
    "doiuse": "4.4.1",
    "dotenv-webpack": "7.1.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "jest-environment-jsdom": "28.1.1",
    "moment-locales-webpack-plugin": "1.2.0",
    "postcss": "8.4.14",
    "postcss-syntax": "0.36.2",
    "react-refresh": "0.13.0",
    "stylelint": "14.9.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended": "8.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "5.0.3",
    "webpack": "5.73.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.2",
    "webpack-merge": "5.8.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "moment": {
      "optional": true
    }
  }
}
