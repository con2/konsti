{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "index.tsx",
  "scripts": {
    "build:ci": "yarn clean && webpack-cli",
    "build:dev": "yarn clean && webpack-cli",
    "build:prod": "yarn clean && webpack-cli",
    "build:staging": "yarn clean && webpack-cli",
    "bundle-analyzer": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json build -m server",
    "clean": "yarn run -T rimraf build",
    "eslint": "yarn run -T eslint . --ext .js,.ts,.tsx",
    "eslint-rules": "yarn run -T eslint --print-config .eslintrc.js",
    "start": "webpack serve",
    "stylelint": "stylelint \"./**/*.{ts,tsx}\"",
    "test": "yarn run -T jest",
    "test:coverage": "yarn run -T jest --coverage --colors",
    "test:watch": "yarn run -T jest --watch",
    "type-check": "yarn run -T tsc --noEmit",
    "playwright": "npx playwright install && npx playwright test --config ./playwright/",
    "playwright:trace": "npx playwright show-trace trace.zip",
    "playwright:debug": "yarn run -T cross-env PWDEBUG=1 npx playwright test --config ./playwright/"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@react-hook/intersection-observer": "3.1.1",
    "axios": "0.25.0",
    "babel-plugin-styled-components": "2.0.2",
    "browserslist": "4.19.1",
    "core-js": "3.20.3",
    "i18next": "21.6.7",
    "i18next-browser-languagedetector": "6.1.2",
    "react": "17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-dom": "17.0.2",
    "react-hook-form": "7.25.0",
    "react-i18next": "11.15.3",
    "react-is": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "5.3.0",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "regenerator-runtime": "0.13.9",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@axe-core/react": "4.3.2",
    "@playwright/test": "1.15.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@types/dotenv-webpack": "7.0.3",
    "@types/moment-locales-webpack-plugin": "1.2.2",
    "@types/react-beautiful-dnd": "13.1.2",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.21",
    "@welldone-software/why-did-you-render": "6.2.3",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "9.2.0",
    "doiuse": "4.4.1",
    "dotenv-webpack": "7.0.3",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "moment-locales-webpack-plugin": "1.2.0",
    "react-refresh": "0.11.0",
    "redux-devtools-extension": "2.13.9",
    "stylelint": "13.13.1",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-no-unsupported-browser-features": "5.0.2",
    "webpack": "5.67.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.3",
    "webpack-merge": "5.8.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "moment": {
      "optional": true
    }
  }
}
