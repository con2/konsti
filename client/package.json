{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "index.tsx",
  "type": "commonjs",
  "scripts": {
    "build:ci": "yarn clean && webpack-cli",
    "build:dev": "yarn clean && webpack-cli",
    "build:kube-dev": "yarn clean && webpack-cli",
    "build:prod": "yarn clean && webpack-cli",
    "build:staging": "yarn clean && webpack-cli",
    "bundle-analyzer": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json build -m server",
    "clean": "yarn run -T rimraf build",
    "eslint": "yarn run -T eslint . --ext .js,.cjs,.ts,.mts,.tsx",
    "eslint-save-config": "eslint --print-config .eslintrc.js > eslint-config-client.json && prettier --write eslint-config-client.json",
    "start": "webpack serve",
    "stylelint": "stylelint \"./**/*.{tsx,ts}\"",
    "stylelint-lint-staged": "stylelint",
    "test": "yarn run -T vitest run",
    "test:coverage": "yarn run -T vitest run --coverage --colors",
    "test:watch": "yarn run -T vitest watch",
    "type-check": "yarn run -T tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-regular-svg-icons": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@reduxjs/toolkit": "2.2.5",
    "@sentry/react": "8.12.0",
    "babel-plugin-styled-components": "2.1.4",
    "browserslist": "4.23.1",
    "core-js": "3.37.1",
    "i18next": "23.11.5",
    "i18next-browser-languagedetector": "8.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.52.0",
    "react-i18next": "14.1.2",
    "react-is": "18.3.1",
    "react-redux": "9.1.2",
    "react-router-dom": "6.24.0",
    "regenerator-runtime": "0.14.1",
    "styled-components": "6.1.11",
    "use-debounce": "10.0.1"
  },
  "devDependencies": {
    "@axe-core/react": "4.9.1",
    "@double-great/stylelint-a11y": "3.0.2",
    "@mdx-js/loader": "3.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
    "@types/dotenv-webpack": "7.0.7",
    "@types/mdx": "2.0.13",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@welldone-software/why-did-you-render": "8.0.3",
    "babel-loader": "9.1.3",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "11.1.0",
    "dotenv-webpack": "8.1.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "jsdom": "24.1.0",
    "postcss": "8.4.38",
    "postcss-styled-syntax": "0.6.4",
    "postcss-syntax": "0.36.2",
    "raw-loader": "4.0.2",
    "react-refresh": "0.14.2",
    "stylelint": "16.6.1",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-no-unsupported-browser-features": "8.0.1",
    "webpack": "5.92.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4",
    "webpack-merge": "5.10.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.12.2"
  }
}
