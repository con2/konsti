{
  "name": "konsti-client",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool client",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "index.tsx",
  "type": "commonjs",
  "scripts": {
    "build:ci": "yarn clean && webpack-cli",
    "build:dev": "yarn clean && webpack-cli",
    "build:kube-dev": "yarn clean && webpack-cli",
    "build:prod": "yarn clean && webpack-cli",
    "build:staging": "yarn clean && webpack-cli",
    "bundle-analyzer": "webpack-cli --profile --json > stats.json && webpack-bundle-analyzer stats.json build -m server",
    "clean": "yarn run -T rimraf build",
    "eslint": "yarn run -T eslint .",
    "start": "webpack serve",
    "stylelint": "stylelint \"./**/*.{tsx,ts}\"",
    "test": "yarn run -T vitest run",
    "test:coverage": "yarn run -T vitest run --coverage --colors",
    "test:watch": "yarn run -T vitest watch",
    "type-check": "yarn run -T tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-regular-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@reduxjs/toolkit": "2.5.0",
    "@sentry/react": "8.48.0",
    "browserslist": "4.24.4",
    "core-js": "3.40.0",
    "i18next": "24.2.1",
    "i18next-browser-languagedetector": "8.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-i18next": "15.4.0",
    "react-redux": "9.2.0",
    "react-router": "7.1.1",
    "regenerator-runtime": "0.14.1",
    "styled-components": "6.1.14",
    "use-debounce": "10.0.4"
  },
  "devDependencies": {
    "@axe-core/react": "4.10.1",
    "@babel/cli": "7.26.4",
    "@babel/core": "7.26.0",
    "@babel/node": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@double-great/stylelint-a11y": "3.0.2",
    "@mdx-js/loader": "3.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
    "@types/dotenv-webpack": "7.0.8",
    "@types/mdx": "2.0.13",
    "@types/react": "19.0.5",
    "@types/react-dom": "19.0.3",
    "@welldone-software/why-did-you-render": "8.0.3",
    "babel-loader": "9.2.1",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-styled-components": "2.1.4",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "11.1.0",
    "dotenv-webpack": "8.1.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.3",
    "jsdom": "25.0.1",
    "postcss": "8.4.49",
    "postcss-styled-syntax": "0.7.0",
    "postcss-syntax": "0.36.2",
    "raw-loader": "4.0.2",
    "react-refresh": "0.16.0",
    "stylelint": "16.13.0",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-no-unsupported-browser-features": "8.0.2",
    "webpack": "5.97.1",
    "webpack-bundle-analyzer": "4.10.2",
