{
  "name": "konsti-server",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool server",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "index.ts",
  "scripts": {
    "eslint": "yarn run -T eslint . --ext .js,.ts",
    "eslint-rules": "yarn run -T eslint --print-config .eslintrc.js",
    "fixer": "ts-node src/features/statistics/dataFixer.ts --",
    "generate-data": "ts-node src/test/test-data-generation/runGenerators.ts",
    "initialize-db": "ts-node src/utils/initializeDatabase.ts",
    "generate-serials": "ts-node src/utils/generateSerials.ts",
    "lint": "yarn eslint && yarn prettier:check",
    "remove-invalid-games": "yarn run -T cross-env NODE_ENV=development ts-node src/test/scripts/removeInvalidGames.ts",
    "start": "ts-node src/index.ts",
    "start:dev": "yarn run -T cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn src/index.ts",
    "stats": "ts-node src/features/statistics/statGenerator.ts --",
    "test": "yarn run -T jest",
    "test-assign": "yarn run -T cross-env NODE_ENV=development ts-node src/test/scripts/testAssignPlayers.ts",
    "test:coverage": "yarn run -T jest --coverage --colors",
    "test:watch": "yarn run -T jest --watch",
    "type-check": "yarn run -T tsc --noEmit",
    "update-game-popularity": "yarn run -T cross-env NODE_ENV=development ts-node src/test/scripts/testUpdateGamePopularity.ts",
    "verify-results": "yarn run -T cross-env NODE_ENV=development ts-node src/test/scripts/testVerifyResults.ts",
    "load-kompassi-data": "yarn run -T cross-env NODE_ENV=development ts-node src/test/scripts/loadKompassiDataToDb.ts"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.2.6"
    }
  },
  "dependencies": {
    "@faker-js/faker": "6.0.0-alpha.3",
    "bcryptjs": "2.4.3",
    "commander": "8.1.0",
    "eventassigner-js": "0.1.6",
    "eventassigner-random": "0.2.0",
    "express": "4.17.1",
    "express-static-gzip": "2.1.1",
    "express-validator": "6.12.1",
    "generate-serial-number": "0.0.3",
    "helmet": "4.6.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.13.7",
    "morgan": "1.10.0",
    "munkres-js": "1.2.2",
    "node-schedule": "2.0.0",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "ts-node": "10.4.0",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.5.5",
    "@types/morgan": "1.9.3",
    "@types/node": "16.7.1",
    "@types/node-schedule": "1.3.2",
    "@types/request-promise-native": "1.0.18",
    "@types/supertest": "2.0.11",
    "mongodb-memory-server": "8.2.0",
    "supertest": "6.2.2",
    "ts-node-dev": "1.1.8"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
