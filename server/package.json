{
  "name": "konsti-server",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool server",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "index.ts",
  "scripts": {
    "build": "yarn build:js",
    "build:js": "babel src -d lib/server/src --extensions \".ts\" --root-mode upward",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf lib",
    "eslint": "eslint . --ext .js,.ts",
    "eslint-rules": "eslint --print-config .eslintrc.js",
    "fixer": "yarn build && node lib/statistics/dataFixer.js --",
    "generate-data": "yarn build && node lib/server/src/test/test-data-generation/runGenerators.js",
    "generate-serials": "yarn build && node lib/utils/generateSerials.js",
    "lint": "yarn eslint && yarn prettier:check",
    "remove-invalid-games": "yarn build && cross-env NODE_ENV=development node lib/test/scripts/removeInvalidGames.js",
    "start": "yarn build && node lib/server/src/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon -e ts,js --exec ts-node -r tsconfig-paths/register --files src/index.ts",
    "stats": "yarn build && node lib/statistics/statGenerator.js --",
    "test": "jest",
    "test-assign": "yarn build && cross-env NODE_ENV=development node lib/test/scripts/testAssignPlayers.js",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "update-game-popularity": "yarn build && cross-env NODE_ENV=development node lib/test/scripts/testUpdateGamePopularity.js",
    "verify-results": "yarn build && cross-env NODE_ENV=development node lib/test/scripts/testVerifyResults.js"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.2.6"
    }
  },
  "dependencies": {
    "array-flat-polyfill": "1.0.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "commander": "7.2.0",
    "eventassigner-js": "0.1.6",
    "eventassigner-random": "0.2.0",
    "express": "4.17.1",
    "express-static-gzip": "2.1.1",
    "express-validator": "6.11.1",
    "generate-serial-number": "0.0.3",
    "helmet": "4.6.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.12.7",
    "morgan": "1.10.0",
    "munkres-js": "1.2.2",
    "node-schedule": "2.0.0",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "runtypes": "6.3.0",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/body-parser": "1.19.0",
    "@types/express": "4.17.11",
    "@types/faker": "5.5.4",
    "@types/jsonwebtoken": "8.5.1",
    "@types/mongoose": "5.10.5",
    "@types/morgan": "1.9.2",
    "@types/node": "15.0.2",
    "@types/node-schedule": "1.3.1",
    "@types/nodemon": "1.19.0",
    "@types/request": "2.48.5",
    "@types/request-promise-native": "1.0.17",
    "@types/supertest": "2.0.11",
    "faker": "5.5.3",
    "mongodb-memory-server": "6.9.6",
    "nodemon": "2.0.7",
    "supertest": "6.1.3"
  }
}
