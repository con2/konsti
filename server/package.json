{
  "name": "konsti-server",
  "version": "0.0.1",
  "private": true,
  "description": "Konsti signup tool server",
  "author": {
    "name": "Arttu Hanska"
  },
  "main": "app.ts",
  "scripts": {
    "build": "yarn build:js",
    "build:js": "babel src -d lib --extensions \".ts\"",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf lib",
    "docker:start": "docker-compose up --detach --build --remove-orphans",
    "docker:stop": "docker-compose down --remove-orphans",
    "eslint": "eslint . --ext .js,.ts",
    "eslint-rules": "eslint --print-config .eslintrc.js",
    "fixer": "yarn build && node lib/statistics/dataFixer.js --",
    "generate-data": "yarn build && node lib/test/test-data-generation/runGenerators.js",
    "generate-serials": "yarn build && node lib/utils/generateSerials.js",
    "lint": "yarn eslint && yarn prettier:check",
    "remove-invalid-games": "yarn build && cross-env NODE_ENV=development node lib/test/scripts/removeInvalidGames.js",
    "start": "yarn build && node lib/app.js",
    "start-dev": "yarn build && cross-env NODE_ENV=development nodemon --ext js,ts,json --exec node lib/app.js",
    "stats": "yarn build && node lib/statistics/statGenerator.js --",
    "test": "jest",
    "test-assign": "yarn build && cross-env NODE_ENV=development node lib/test/scripts/testAssignPlayers.js",
    "test:coverage": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "update-game-popularity": "yarn build && cross-env NODE_ENV=development node lib/test/scripts/testUpdateGamePopularity.js",
    "verify-results": "yarn build && cross-env NODE_ENV=development node lib/test/scripts/testVerifyResults.js"
  },
  "config": {
    "mongodbMemoryServer": {
      "version": "4.2.6"
    }
  },
  "dependencies": {
    "array-flat-polyfill": "1.0.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "commander": "6.1.0",
    "eventassigner-js": "0.1.6",
    "express": "4.17.1",
    "express-jwt": "6.0.0",
    "express-static-gzip": "2.0.8",
    "express-validator": "6.6.1",
    "generate-serial-number": "0.0.3",
    "helmet": "4.1.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.10.2",
    "morgan": "1.10.0",
    "munkres-js": "1.2.2",
    "node-schedule": "1.3.2",
    "request": "2.88.2",
    "request-promise-native": "1.0.9",
    "runtypes": "5.0.1",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/body-parser": "1.19.0",
    "@types/express": "4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/faker": "4.1.12",
    "@types/jsonwebtoken": "8.5.0",
    "@types/mongoose": "5.7.36",
    "@types/morgan": "1.9.1",
    "@types/node": "14.6.3",
    "@types/node-schedule": "1.3.0",
    "@types/nodemon": "1.19.0",
    "@types/request": "2.48.5",
    "@types/request-promise-native": "1.0.17",
    "@types/supertest": "2.0.10",
    "faker": "5.1.0",
    "mongodb-memory-server": "6.6.7",
    "nodemon": "2.0.4",
    "supertest": "4.0.2"
  }
}
